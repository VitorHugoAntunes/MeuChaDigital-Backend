{
  "name": "meuchadigital",
  "version": "1.0.0",
  "main": "server.js",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon --exec ts-node -r dotenv/config src/server.ts dotenv_config_path=.env.development",
    "prod": "cross-env NODE_ENV=production node -r dotenv/config dist/server.js dotenv_config_path=.env.production",
    "prisma:generate:dev": "dotenv -e .env.development -- npx prisma generate",
    "prisma:generate:prod": "dotenv -e .env.production -- npx prisma generate",
    "prisma:generate:local": "dotenv -e .env.local -- npx prisma generate",
    "prisma:migrate:dev": "dotenv -e .env.development -- npx prisma migrate dev",
    "prisma:migrate:prod": "dotenv -e .env.production -- npx prisma migrate deploy",
    "studio:dev": "dotenv -e .env.development -- npx prisma studio",
    "studio:prod": "dotenv -e .env.production -- npx prisma studio",
    "start": "cross-env NODE_ENV=production node -r dotenv/config dist/server.js dotenv_config_path=.env.production"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.750.0",
    "@prisma/client": "^6.3.0",
    "@types/express": "^5.0.0",
    "axios": "^1.7.9",
    "bcrypt": "^5.1.1",
    "cookie-session": "^2.1.0",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "express-session": "^1.17.2",
    "mercadopago": "^2.3.0",
    "meuchadigital": "file:",
    "multer": "^1.4.3",
    "multer-s3": "^3.0.1",
    "passport": "^0.5.3",
    "passport-google-oauth2": "^0.2.0",
    "prisma": "^6.3.0",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.0.5",
    "yup": "^1.6.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-session": "^2.0.49",
    "@types/cors": "^2.8.17",
    "@types/express-session": "^1.18.1",
    "@types/multer": "^1.4.12",
    "@types/multer-s3": "^3.0.3",
    "@types/node": "^22.13.0",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth2": "^0.1.10",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.7",
    "dotenv-cli": "^8.0.0",
    "nodemon": "^3.1.9",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  }
}
